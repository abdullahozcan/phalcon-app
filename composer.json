{
  "name": "phalcon/phalcon",
  "description": "Phalcon Framework",
  "keywords": [
    "php",
    "framework",
    "psr-7",
    "psr-15",
    "psr-17"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-phalcon": "^5.0",
    "ext-redis": "*",
    "phalcon/dd": "^2.0",
    "symfony/yaml": "^6.2",
    "robmorgan/phinx": "^0.13.3"
  },
  "require-dev": {
    "phalcon/ide-stubs": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
